{
  "expo": {
    "plugins": [
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/Inter-Black.otf", "./assets/fonts/Inter-SemiBold.otf", "./assets/fonts/Inter-Medium.otf"]
        }
      ]
    ],
    "name": "capstone",
    "slug": "capstone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/LogoNoBackground.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/LogoNoBackground.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/LogoNoBackground.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./assets/images/LogoNoBackground.png"
    }
  }
}
